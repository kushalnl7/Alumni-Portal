<% layout('layouts/boilerplate') %>

  <head>
    <title>Landing Page</title>
    <link rel="stylesheet" type="text/css" href="/css/home.css">
    </link>
  </head>

  <body>
    <div class="container">
      <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel" style="padding: 0%; margin:0%;">
        <ol class="carousel-indicators">
          <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>

        </ol>
        <div class="carousel-inner">
          <div class="carousel-item active" style="background: rgba(0, 0, 0, 1);">
            <img class="d-block w-100" height="650px" src="/pictures/coepg.jpg" alt="First slide">
            <div class="carousel-caption d-none d-md-block">
              <h5 style="color: white; font-weight: bold; font-size: 30px;">College of Engineering Pune</h5>
            </div>
          </div>
          <div class="carousel-item ">
            <img class="d-block w-100" height="650px" src="/pictures/Ganga.jpg" alt="Second slide">
            <div class="carousel-caption d-none d-md-block">
              <h5 style="color: black; font-weight: bold; font-size: 30px;">Ganga Institue of Technology</h5>
            </div>
          </div>
          <div class="carousel-item ">
            <img class="d-block w-100" height="650px" src="/pictures/BITS.jpg" alt="Third slide">
            <div class="carousel-caption d-none d-md-block">
              <h5 style="color: white; font-weight: bold; font-size: 30px;">BITS Pilani</h5>
            </div>
          </div>
          <div class="carousel-item ">
            <img class="d-block w-100" height="650px" src="/pictures/mit-wpu-pune.jpg" alt="Fourth slide">
            <div class="carousel-caption d-none d-md-block">
              <h5 style="color: white; font-weight: bold; font-size: 30px;">MIT Pune</h5>
            </div>
          </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </a>
      </div>
    </div>
    <br>
    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <div class="card text-white bg-dark mb-3">
            <div class="card-body">
              <h2 class="card-title" style="padding: 2%;">Our Mission</h2>
              <p class="card-text">We enrich the Universities' impact by serving as an independent gateway for alums
                of all identities, backgrounds, and experiences – across the globe and in our neighborhoods –
                in order to foster a life-long connection between the college and its graduates
                and create and deepen belonging to the Same Family, the Leaders and Best.</p>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="card text-white bg-dark mb-3">
            <div class="card-body">
              <h2 class="card-title" style="padding: 2%;">Join Us</h2>
              <a href="/collegeregister" class="btn btn-secondary">Register as College</a>
              <a href="/alumniregister" class="btn btn-secondary">Register as Alumni</a>
              <p class="card-text m-t-40">Already have an account?</p>
              <a href="/login" class="btn btn-secondary">Login Here</a>

            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <h1 style="text-align-last: center; padding: 2%;">Upcoming Virtual Events</h2>
          <div class="col-sm-4">
            <div class="card text-white bg-dark mb-3">
              <div class="card-body">
                <h5 class="card-title"><img src="/pictures/calendar.ico" width="40px" style="padding: 1%;" /> 20
                  December, 2020</h5>
                <h6 class="card-text">College of Engineering, Pune</h6>
                <p class="card-text">LinkedIn Profile Review Session by Mr.Bajaj</p>
                <a href="https://add.eventable.com/events/5fd9cc567612870015f3c781/5fd9cc5832d4730016b69fa6"
                  class="eventable-link btn btn-secondary" target="_blank" data-key="5fd9cc567612870015f3c781"
                  data-event="5fd9cc5832d4730016b69fa6" data-style="1">Add to Calendar</a>
                <script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = 'https://plugins.eventable.com/eventable.js'; fjs.parentNode.insertBefore(js, fjs); } }(document, 'script', 'eventable-script');</script>
              </div>
            </div>
          </div>
          <div class="col-sm-4">
            <div class="card text-white bg-dark mb-3">
              <div class="card-body">
                <h5 class="card-title"><img src="/pictures/calendar.ico" width="40px" style="padding: 1%;" /> 24
                  December, 2020</h5>
                <h6 class="card-text">Ganga Institue of Technology</h6>
                <p class="card-text">Is Grad School a Good Move For My Career? </p>
                <a href="https://add.eventable.com/events/5fd9cc567612870015f3c781/5fd9cc5832d4730016b69fa6"
                  class="eventable-link btn btn-secondary" target="_blank" data-key="5fd9cc567612870015f3c781"
                  data-event="5fd9cc5832d4730016b69fa6" data-style="1">Add to Calendar</a>
                <script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = 'https://plugins.eventable.com/eventable.js'; fjs.parentNode.insertBefore(js, fjs); } }(document, 'script', 'eventable-script');</script>
              </div>
            </div>
          </div>
          <div class="col-sm-4">
            <div class="card text-white bg-dark mb-3">
              <div class="card-body">
                <h5 class="card-title"><img src="/pictures/calendar.ico" width="40px" style="padding: 1%;" /> 26
                  December, 2020</h5>
                <h6 class="card-text">MIT-WPU, Pune</h6>
                <p class="card-text">Communication & Life Skills you must know</p>
                <a href="https://add.eventable.com/events/5fd9cc567612870015f3c781/5fd9cc5832d4730016b69fa6"
                  class="eventable-link btn btn-secondary" target="_blank" data-key="5fd9cc567612870015f3c781"
                  data-event="5fd9cc5832d4730016b69fa6" data-style="1">Add to Calendar</a>
                <script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = 'https://plugins.eventable.com/eventable.js'; fjs.parentNode.insertBefore(js, fjs); } }(document, 'script', 'eventable-script');</script>
              </div>
            </div>
          </div>
      </div>
    </div>
    <section class="stat" id="stats">
      <div class="content-box">
        <br><br>
        <div class="container" style="padding-bottom: 50px;">
          <div class="row text-center">
            <div class="col-md-6">
              <div class="stat-items">
                <!-- <i class="bi bi-people"></i>-->
                <svg xmlns="http://www.w3.org/2000/svg" width="60" height="60" fill="currentColor"
                  class="bi bi-person-check" viewBox="0 0 16 16">
                  <path fill-rule="evenodd"
                    d="M8 5a2 2 0 1 1-4 0 2 2 0 0 1 4 0zM6 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6zm6 5c0 1-1 1-1 1H1s-1 0-1-1 1-4 6-4 6 3 6 4zm-1-.004c-.001-.246-.154-.986-.832-1.664C9.516 10.68 8.289 10 6 10c-2.29 0-3.516.68-4.168 1.332-.678.678-.83 1.418-.832 1.664h10zm4.854-7.85a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0l-1.5-1.5a.5.5 0 0 1 .708-.708L12.5 7.793l2.646-2.647a.5.5 0 0 1 .708 0z" />
                </svg>

                <h2><span class="counter text-center">
                    <%=collegecount%>
                  </span> <span>+</span></h2>
                <p>Colleges Registered!</p>
              </div>
            </div>

            <div class="col-md-6">
              <div class="stat-items">
                <!-- <i class="fa fa-camera"></i> -->
                <svg xmlns="http://www.w3.org/2000/svg" width="60" height="60" fill="currentColor" class="bi bi-people"
                  viewBox="0 0 16 16">
                  <path fill-rule="evenodd"
                    d="M15 14s1 0 1-1-1-4-5-4-5 3-5 4 1 1 1 1h8zm-7.978-1h7.956a.274.274 0 0 0 .014-.002l.008-.002c-.002-.264-.167-1.03-.76-1.72C13.688 10.629 12.718 10 11 10c-1.717 0-2.687.63-3.24 1.276-.593.69-.759 1.457-.76 1.72a1.05 1.05 0 0 0 .022.004zM11 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4zm3-2a3 3 0 1 1-6 0 3 3 0 0 1 6 0zM6.936 9.28a5.88 5.88 0 0 0-1.23-.247A7.35 7.35 0 0 0 5 9c-4 0-5 3-5 4 0 .667.333 1 1 1h4.216A2.238 2.238 0 0 1 5 13c0-1.01.377-2.042 1.09-2.904.243-.294.526-.569.846-.816zM4.92 10c-1.668.02-2.615.64-3.16 1.276C1.163 11.97 1 12.739 1 13h3c0-1.045.323-2.086.92-3zM1.5 5.5a3 3 0 1 1 6 0 3 3 0 0 1-6 0zm3-2a2 2 0 1 0 0 4 2 2 0 0 0 0-4z" />
                </svg>
                <!-- <i class="fas fa-user-friends fa-2x" style="color:red"></i> -->

                <h2><span class="counter text-center">
                    <%=alumnicount%>
                  </span> <span>+</span></h2>
                <p>Alumni Registered</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

  </body>